{
  "name": "@acoyfellow/anytool",
  "version": "2.0.0",
  "description": "Dynamic tool generation pattern - generate and execute tools on demand",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "ai": "^5.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "zod": "^4.0.0"
  },
  "keywords": [
    "ai",
    "tools",
    "dynamic",
    "code-generation",
    "cloudflare-workers",
    "dynamic-code-generation",
    "worker-loaders",
    "openai"
  ],
  "author": "acoyfellow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acoyfellow/anytool"
  }
}


